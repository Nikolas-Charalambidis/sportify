		"prettier": "prettier --write \"src/**/*.js\" README.md"
	},
	"dependencies": {
		"@sendgrid/mail": "^6.4.0",
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"cloudinary": "^1.16.0",
		"connect-multiparty": "^2.2.0",
		"cors": "^2.8.5",
		"date-fns": "^2.8.1",
		"dotenv": "^8.1.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"mariadb": "^2.1.2",
		"multer": "^1.4.2",
		"multer-storage-cloudinary": "^2.2.1",
		"serialize-javascript": "2.1.1",
		"swagger-jsdoc": "^3.4.0",
		"swagger-ui-express": "^4.1.2"
	},
	"devDependencies": {
		"backpack-core": "^0.8.4",
		"nodemon": "1.19.3",
		"npm-run-all": "4.1.5",
