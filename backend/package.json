{
	"name": "backend",
	"version": "0.1.0",
	"private": true,
	"main": "build/main.js",
	"scripts": {
		"dev": "backpack",
		"dev:debug": "backpack dev --inspect --debug-brk",
		"start": "node ./build/main.js",
		"prestart": "run-s build",
		"build": "backpack build",
		"build:watch": "nodemon --watch ./src --exec 'npm run build'",
		"postbuild": "nodetouch tmp/restart.txt",
		"prettier": "prettier --write \"src/**/*.js\" README.md"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"dotenv": "^8.1.0",
		"express": "^4.17.1",
		"swagger-jsdoc": "^3.4.0",
		"swagger-ui-express": "^4.1.2",
		"mariadb": "^2.1.2",
		"bcrypt": "^3.0.6",
		"nodemailer": "^6.3.1",
		"sendmail": "^1.6.1"
	},
	"devDependencies": {
		"backpack-core": "^0.8.4",
		"nodemon": "1.19.3",
		"npm-run-all": "4.1.5",
		"prettier": "1.18.2",
		"touch": "3.1.0"
	}
}
